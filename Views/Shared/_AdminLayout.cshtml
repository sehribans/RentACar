                            </ul>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="~/Admin/MarkaListe" class="nav-link">
                                        <i class="fa-light fa-clipboard-list"></i>
                                        <p>Marka Listesi</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
